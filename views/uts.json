{
  "title": "UTS Timetable View",
  "filter": [
    ["start", "<=", "now"]
  ],
  "variables": {
    "nclass": 0
  },
  "replacers": {
    "abbr": [
      ["/\\bIntroduction\\b/", "Int."],
      ["/\\bMathematical\\b/", "Math."],
      ["/\\bDatabase\\b/", "DB"]
    ],
    "ordinal_letter": [
      ["/1/", "A"],
      ["/2/", "B"]
    ],
    "activity": [
      ["/^lec$/mi", "Lecture"],
      ["/^tut$/mi", "Tutorial"],
      ["/^cmp$/mi", "Computer Lab"],
      ["/^wrk$/mi", "Workshop"]
    ],
    "room": [
      ["/ONLINE\\d*/", "Online"],
      ["/^(?! *online)/i", "in "]
    ],
    "duration": [
      ["/.+(\\d+h)(\\d+m).+/", "$1 $2"]
    ],
    "time": [
      ["/\\d+-\\d+-\\d+ (\\d+:\\d+):\\d+/", "$1 am"],
      ["/13(:\\d+) am/", "1$1 pm"],
      ["/14(:\\d+) am/", "2$1 pm"],
      ["/15(:\\d+) am/", "3$1 pm"],
      ["/16(:\\d+) am/", "4$1 pm"],
      ["/17(:\\d+) am/", "5$1 pm"],
      ["/18(:\\d+) am/", "6$1 pm"],
      ["/19(:\\d+) am/", "7$1 pm"],
      ["/20(:\\d+) am/", "8$1 pm"],
      ["/21(:\\d+) am/", "9$1 pm"],
      ["/22(:\\d+) am/", "10$1 pm"],
      ["/23(:\\d+) am/", "11$1 pm"]
    ]
  },
  "shaders": {
    "desc": "/([0-9]+)_([A-Z]+)_([A-Z]+)_([0-9])_([A-Z]+), ([A-Za-z]+)([0-9]+), ([0-9]+)\\n(.+)/"
  },
  "article": [
    [
      ["<b>", ["shader", ["var", "_event.description"], "desc", 0, 1]],
      " ",
      ["replace", ["shader", ["var", "_event.description"], "desc", 0, 9], "abbr"]
    ],
    [
      ["replace", ["shader", ["var", "_event.description"], "desc", 0, 6], "activity"],
      " for ",
      ["replace", ["var", "_event.duration"], "duration"]
    ],
    [
      "@ ",
      ["replace", ["var", "_event.start"], "time"],
      " ",
      ["replace", ["var", "_event.location"], "room"]
    ]
  ]
}